{
  "name": "GamifyMath",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/ReplicatedStorage"
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/ServerScriptService"
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "src/StarterPlayerScripts"
      },
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts",
        "$path": "src/StarterCharacterScripts"
      }

    },
      "Handlers":{
        "$className":"Handlers",
        "$path":"src/StarterPlayer/StarterPlayerScripts/Handlers"
    },
    "Inventory":{
      "$className": "Inventory",
      "$path": "src/StartPlayer/StarterPlayerScripts/Inventory"
    },
    "LightingSystem": {
      "$className": "LightingSystem",
      "$path":"src/StarterPlayer/StarterPlayerScripts/Handlers/LightingSystem"
    },
    

    "Workspace": {
      "$className": "Workspace",
      "$ignoreUnknownInstances": true,
      "$properties": {
        "FilteringEnabled": true
      }
    }
  }
}